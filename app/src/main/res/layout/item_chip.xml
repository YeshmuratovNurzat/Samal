    app:chipBackgroundColor="@color/chip_bg"
    app:chipCornerRadius="16dp"
    app:chipMinTouchTargetSize="0dp"
    app:chipStrokeWidth="0.5dp"
    app:rippleColor="@null"
    tools:text="adsd" />
